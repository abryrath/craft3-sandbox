{
    "require": {
        "craftcms/cms": "3.1.27",
        "vlucas/phpdotenv": "^2.4.0",
        "unionco/import": "dev-master",
        "unionco/google-services": "@dev",
        "league/csv": "^8",
        "ether/simplemap": "3.4.11",
        "craftcms/commerce": "2.1.2",
        "unionco/core": "^3.13",
        "monolog/monolog": "^1.24",
        "scrivo/highlight.php": "v9.15.6.1",
        "unionco/syncdb": "@dev",
        "unionco/craft-sync-db": "@dev",
        "unionco/craft-related-entry-types": "@dev",
        "tsingsun/yii2-graphql": "^0.11.4"
    },
    "autoload": {
        "psr-4": {
            "unionco\\app\\": "modules/app/src"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "platform": {
            "php": "7.2"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "repositories": [{
            "type": "path",
            "url": "./import",
            "symlink": true
        },
        {
            "type": "path",
            "url": "../../Library/craft-sync-db",
            "symlink": true
        },
        {
            "type": "path",
            "url": "../../Library/syncdb"
        },
        {
            "type": "path",
            "url": "../../Library/relatedentrytypes"
        },
        {
            "type": "path",
            "url": "./googleservices"
        },
        {
            "symlink": true,
            "type": "path",
            "url": "../../Library/geolocation/"
        },
        {
            "symlink": true,
            "type": "path",
            "url": "../../Library/core"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "vimeo/psalm": "^3.2",
        "squizlabs/php_codesniffer": "^3.4"
    }
}
